import java.io.*;
public class BRRead {
	public static void main(String[] args) 
		throws IOException{ 
		char c; 
		BufferReader br = new
			BufferedReader(new InputStream(System.in));
		System.out.println("Enter characters, 'q' tp quit");
		do {
			c = (char) br.read();
			System.out.println(c);
		} while (c != 'q');
	}
}
